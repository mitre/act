{
  "name": "nuxt-ui-pro-template-saas",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:log": "nuxt dev 2>&1 | tee logs/dev-$(date +%Y%m%d-%H%M%S).log",
    "dev:clean": "nuxt dev --clean-cache",
    "dev:clean-log": "nuxt dev --clean-cache 2>&1 | tee logs/dev-clean-$(date +%Y%m%d-%H%M%S).log",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "logs:clear": "rm -rf logs/*.log || true",
    "logs:dir": "mkdir -p logs",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "pre-push": "pnpm typecheck"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{md,yml,yaml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.45",
    "@iconify-json/simple-icons": "^1.2.37",
    "@nuxt/content": "https://pkg.pr.new/@nuxt/content@bf24dfe",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.13.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "^3.1.3",
    "@nuxt/ui-pro": "^3.1.3",
    "@standard-schema/spec": "^1.0.0",
    "@vueuse/nuxt": "^13.3.0",
    "defu": "^6.1.4",
    "nuxt": "^3.17.5",
    "nuxt-github-pages": "^1.5.0",
    "nuxt-og-image": "^5.1.5",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/vscode-icons": "^1.2.22",
    "@nuxt/eslint": "^1.4.1",
    "eslint": "^9.28.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "sharp": "^0.34.2",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3",
    "vue-tsc": "2.2.10"
  },
  "resolutions": {
    "unimport": "4.1.1",
    "vue-tsc": "2.2.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "packageManager": "pnpm@10.7.0"
}
